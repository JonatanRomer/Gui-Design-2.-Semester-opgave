    xmlns:local="using:Gui_Design_2.Semester_opgave"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <Rectangle Fill="#59256FB4" HorizontalAlignment="Left" Height="60" Stroke="Black" VerticalAlignment="Top" Width="432"/>
        <Button x:Name="button" Content="≡" HorizontalAlignment="Left" VerticalAlignment="Top" Height="60" Padding="0,0,0,14" Click="Buttom_Click_1" Background="#00000000" FontSize="40" Margin="347,0,0,0" Width="85"/>
        <Button x:Name="button1" Content="+ Opret Barn" HorizontalAlignment="Left" Margin="35,579,0,0" VerticalAlignment="Top" Height="100" Width="360" Click="Buttom_Click_2" RenderTransformOrigin="0.5,0.5" Background="#59256FB4" FontSize="40">

        </Button>
        <Image x:Name="image" HorizontalAlignment="Left" Height="189" Margin="35,248,0,0" VerticalAlignment="Top" Width="360" Visibility="Visible" Source="Assets/stort_logo.png">
            <Image.FocusVisualPrimaryBrush>
                <ImageBrush Stretch="Fill"/>
            </Image.FocusVisualPrimaryBrush>
        </Image>

    </Grid>
</Page>

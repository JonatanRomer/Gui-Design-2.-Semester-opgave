        <Button x:Name="button_Copy" Content="←" HorizontalAlignment="Left" VerticalAlignment="Top" Padding="0,0,0,14" Height="60" Width="85" Click="Buttom_Click_2" Background="#00000000" FontSize="40"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Margin="129,5,0,0" TextWrapping="Wrap" Text="   Historik" VerticalAlignment="Top" Width="150" Height="50" FontSize="30" TextChanged="textBox_TextChanged" Background="Transparent" BorderBrush="#007A7A7A"/>


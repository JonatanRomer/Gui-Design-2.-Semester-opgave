        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Margin="39,517,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Height="44" Width="351"/>
        <TextBlock x:Name="textBlock1" HorizontalAlignment="Left" Margin="81,473,0,0" TextWrapping="Wrap" Text="Opret givet vaccination:" VerticalAlignment="Top" Height="44" Width="301" FontSize="25"/>
        <TextBlock x:Name="textBlock2" HorizontalAlignment="Left" Margin="39,561,0,0" TextWrapping="Wrap" Text="(*F.eks rejsevaccinationer*)" VerticalAlignment="Top" Height="22" Width="317" FontSize="15"/>
